<schema adapter="tagabukid_hrmis">
	 <element tablename="pmis_ipcr_behavioral_items">
		<field name="objid" primary="true" prefix="IPBI"/> 
		<field name="parentid" required="true" /> 
		<field name="behavioralid" required="true" /> 
		<field name="rating" /> 
		<field name="remarks" /> 
		<complex name="parentbehavioraltype" ref="pmis_ipcr_behavioral" jointype="many-to-one" includefields="objid,type" required="false">
			<key field="parentid" target="objid" />
		</complex>
	</element>
</schema>
